<footer id="colophon" class="site-footer">
  <div class="top-footer footer-column-4">
    <div class="mid-content">
      <div class="top-footer-wrap clearfix">
        <div class="footer-block">
          <aside id="googlemapswidget-2" class="widget google-maps-widget" style="height: 175px;">
            <h2 class="widget-title">Map</h2>
            <div id="map" style="height: 100%"></div>
          </aside>
          <aside id="text-13" class="widget widget_text">
            <h2 class="widget-title">Follow me :</h2>
            <div class="textwidget"></div>
          </aside>
          <aside id="sfsi-plus-widget-3" class="widget sfsi_plus">
            <div class="sfsi_plus_widget" data-position="widget" style="min-height: auto;">
              <div id="sfsi_plus_wDiv"></div>
              <script>
              window.addEventListener('sfsi_plus_functions_loaded', function() {
                if (typeof sfsi_plus_widget_set == 'function') {
                  sfsi_plus_widget_set();
                }
              })
              </script>
              <div class="sfsiplus_norm_row sfsi_plus_wDiv" style="width:450px;position:absolute;;text-align:left; display:flex">
                <div style="width:40px; height:40px;margin-left:5px;margin-bottom:5px;" class="sfsi_plus_wicons shuffeldiv ">
                  <div class="sfsiplus_inerCnt">
                    <a class=" sficn" data-effect="combo" target="_blank" href="<%= setting(:facebook) %>" style="opacity:0.6"><img alt="Facebook" title="Facebook" src="https://collievn.com/wp-content/plugins/ultimate-social-media-plus/images/icons_theme/cute/cute_fb.png" width="40" height="40" style="" class="sfcm sfsi_wicon sfsiplusid_round_icon_facebook" data-effect="combo"></a>
                  </div>
                </div>
                <div style="width:40px; height:40px;margin-left:5px;margin-bottom:5px;" class="sfsi_plus_wicons shuffeldiv ">
                  <div class="sfsiplus_inerCnt">
                    <a class=" sficn" data-effect="combo" target="_blank" href="<%= setting(:twitter) %>" style="opacity:0.6"><img alt="Twitter" title="Twitter" src="https://collievn.com/wp-content/plugins/ultimate-social-media-plus/images/icons_theme/cute/cute_twitter.png" width="40" height="40" style="" class="sfcm sfsi_wicon sfsiplusid_round_icon_twitter" data-effect="combo"></a>
                  </div>
                </div>
                <div style="width:40px; height:40px;margin-left:5px;margin-bottom:5px;" class="sfsi_plus_wicons shuffeldiv ">
                  <div class="sfsiplus_inerCnt">
                    <a class=" sficn" data-effect="combo" target="_blank" href="<%= setting(:youtube) %>" style="opacity:0.6"><img alt="YouTube" title="YouTube" src="https://collievn.com/wp-content/plugins/ultimate-social-media-plus/images/icons_theme/cute/cute_youtube.png" width="40" height="40" style="" class="sfcm sfsi_wicon sfsiplusid_round_icon_youtube" data-effect="combo"></a>
                  </div>
                </div>
                <div style="width:40px; height:40px;margin-left:5px;margin-bottom:5px;" class="sfsi_plus_wicons shuffeldiv ">
                  <div class="sfsiplus_inerCnt">
                    <a class=" sficn" data-effect="combo" target="_blank" href="<%= setting(:pinterest) %>" style="opacity:0.6"><img alt="PINTEREST" title="PINTEREST" src="https://collievn.com/wp-content/plugins/ultimate-social-media-plus/images/icons_theme/cute/cute_pinterest.png" width="40" height="40" style="" class="sfcm sfsi_wicon sfsiplusid_round_icon_pinterest" data-effect="combo"></a>
                  </div>
                </div>
                <div style="width:40px; height:40px;margin-left:5px;margin-bottom:5px;" class="sfsi_plus_wicons shuffeldiv ">
                  <div class="sfsiplus_inerCnt">
                    <a class=" sficn" data-effect="combo" target="_blank" href="<%= setting(:instagram) %>" style="opacity:0.6"><img alt="INSTAGRAM" title="INSTAGRAM" src="https://collievn.com/wp-content/plugins/ultimate-social-media-plus/images/icons_theme/cute/cute_instagram.png" width="40" height="40" style="" class="sfcm sfsi_wicon sfsiplusid_round_icon_instagram" data-effect="combo"></a>
                  </div>
                </div>
              </div>
              <div id="sfsi_holder" class="sfsi_plus_holders" style="position: relative; float: left; width: 100%; z-index: -1; height: 66px;"></div>
              <script></script>
              <div style="clear: both;"></div>
            </div>
          </aside>
        </div>
        <div class="footer-block">
          <aside id="text-5" class="widget widget_text">
            <h2 class="widget-title">About me</h2>
            <div class="textwidget">
              <%= setting(:about_me) %>
            </div>
          </aside>
          <aside id="text-15" class="widget widget_text">
            <h2 class="widget-title">Chính sách khách hàng</h2>
            <div class="textwidget">
              <%= setting(:policy) %>
            </div>
          </aside>
        </div>
        <div class="footer-block">
          <aside id="text-20" class="widget widget_text">
            <h2 class="widget-title">LIÊN HỆ :</h2>
            <div class="textwidget">
              <%= setting(:contact) %>
            </div>
          </aside>
        </div>
        <div class="footer-block">
          <aside id="sfp_page_plugin_widget-5" class="widget widget_sfp_page_plugin_widget">
            <h2 class="widget-title">Fanpage :</h2>
            <div id="fb-root" class=" fb_reset">
              <div class="fb-page" 
                data-href="<%= setting(:fanpage) %>"
                data-width="380" 
                data-hide-cover="false"
                data-show-facepile="false"></div>
            </div>
          </aside>
          <aside id="media_image-6" class="widget widget_media_image">
            <h2 class="widget-title">CHỨNG NHẬN</h2>
            <%= setting(:certification) %>
          </aside>
        </div>
      </div>
    </div>
  </div>
  <div class="bottom-footer">
    <div class="mid-content clearfix">
      <div class="copy-right">
        © 2020 Beagle Việt Nam
      </div>
      <!-- .copy-right -->
      <div class="site-info"></div>
      <!-- .site-info -->
    </div>
  </div>
</footer>